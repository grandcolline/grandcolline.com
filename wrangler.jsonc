{
  "name": "grandcollinecom",
  "compatibility_date": "2025-06-12",
  "main": "./dist/_worker.js/index.js",
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist"
  },
  "workers_dev": true,
  "preview_urls": true,
  "compatibility_flags": ["nodejs_compat", "global_fetch_strictly_public"],

  "env": {
    // ---------------------------
    // Production 設定
    // ---------------------------
    "production": {
      "compatibility_date": "2025-06-12",
      "zone_id": "784eac4a732fe8044f13d657311f082b",
      "routes": [
        {
          "pattern": "grandcolline.com",
          "custom_domain": true
        },
        {
          "pattern": "www.grandcolline.com",
          "custom_domain": true
        }
      ]
    }
  }
}
